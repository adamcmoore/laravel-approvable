{
    "name": "adamcmoore/laravel-approvable",
    "description": "Require approval of changes to eloquent models",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Moore",
            "email": "adam@acmoore.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10",
        "illuminate/database": "^10",
        "illuminate/filesystem": "^10"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "phpunit/phpunit": "^10",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^8.5",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "AcMoore\\Approvable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcMoore\\Approvable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AcMoore\\Approvable\\ApprovableServiceProvider"
            ]
        }
    }
}
