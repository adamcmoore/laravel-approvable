{
    "name": "adamcmoore/laravel-approvable",
    "description": "Require approval of changes to eloquent models",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Moore",
            "email": "adam@acmoore.co.uk"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": ">=3.5",
        "orchestra/database": ">=3.5"
    },
    "autoload": {
        "psr-4": {
            "AcMoore\\Approvable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcMoore\\Approvable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AcMoore\\Approvable\\ApprovableServiceProvider"
            ]
        }
    }
}
